<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ramapo.edu.neupanemexicantrain">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NeupaneMexicanTrain">
        <activity
        android:name="ramapo.edu.neupanemexicantrain.controller.RoundActivity"
        android:exported="false" />
        <activity
            android:name="ramapo.edu.neupanemexicantrain.controller.Menu"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ramapo.edu.neupanemexicantrain.controller.PlayGame"
            android:exported="false" />
    </application>

</manifest>